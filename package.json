{
  "name": "ursiform",
  "version": "0.0.8",
  "description": "Client library for the Ursiform API",
  "homepage": "https://github.com/ursiform/ursiform-js",
  "author": "A. Darian <git@darian.af>",
  "main": "lib/ursiform.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ursiform/ursiform-js.git"
  },
  "keywords": [
    "ursiform"
  ],
  "scripts": {
    "agent": "AGENT=`node ./scripts/agent.js` && sed -i.b \"s|USERAGENT|${AGENT}|g\" ./lib/ursiform.js && rm ./lib/*.b",
    "compile": "babel --optional runtime --source-maps --out-dir lib/ src/ && npm run agent",
    "prepublish": "npm run compile"
  },
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "eventemitter3": "^1.1.1",
    "superagent": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.20"
  },
  "userAgent": "ursiform-js"
}
